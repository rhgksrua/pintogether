  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "connect-mongo": "^1.3.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "mongoose": "^4.6.0",
    "morgan": "^1.7.0",
