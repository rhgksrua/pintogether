{
  "name": "reactboilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for react redux router express",
  "main": "index.js",
  "scripts": {
    "dev:client": "webpack --watch --config webpack.client.config.js",
    "dev:server": "webpack --watch --config webpack.server.config.js",
    "dev:watch": "npm run dev:client & npm run dev:server & nodemon server/build/serverBundle.js -V --watch server/build/serverBundle.js",
    "test": "mocha --compilers js:babel-core/register ./server/server_spec.js"
  },
